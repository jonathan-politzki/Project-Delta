{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "devCommand": "npm start",
    "framework": "create-react-app",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" }
    ]
  }
  